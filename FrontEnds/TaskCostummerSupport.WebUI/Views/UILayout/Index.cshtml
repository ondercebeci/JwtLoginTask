
@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width,initial-scale=1">
    <title>Customer Support</title>
    <!-- Favicon icon -->
    <link rel="icon" type="~/adminweb/image/png" sizes="16x16" href="~/adminweb/images/l-dark.png">
    <!-- Custom Stylesheet -->
    <link href="~/adminweb/css/style.css" rel="stylesheet">
</head>
<body>

    <div id="preloader">
        <div class="loader">
            <svg class="circular" viewBox="25 25 50 50">
                <circle class="path" cx="50" cy="50" r="20" fill="none" stroke-width="3" stroke-miterlimit="10" />
            </svg>
        </div>
    </div>
   
    <div id="main-wrapper">
        <div class="nav-header">
            <div class="brand-logo">
                <a href="index.html">
                    <b class="logo-abbr"><img src="~/adminweb/images/logo.png" alt=""> </b>
                    <span class="logo-compact"><img src="~/adminweb/images/logo-compact.png" alt=""></span>
                    <span class="brand-title">
                        <img src="~/adminweb/images/logo-text.png" alt="">
                    </span>
                </a>
            </div>
        </div>
        <div class="header">
            <div class="header-content clearfix">

                <div class="nav-control">
                    <div class="hamburger">
                        <span class="toggle-icon"><i class="icon-menu"></i></span>
                    </div>
                </div>
                <div class="header-left">
                    
                </div>
                
            </div>
        </div>
        <div class="nk-sidebar">
            <div class="nk-nav-scroll">
                <ul class="metismenu" id="menu">
                    <li class="nav-label">Dashboard</li>
                    
                    <li>
                        <a href="/Contact/Index/" aria-expanded="false">
                            <i class="icon-badge menu-icon"></i><span class="nav-text">İletişim Formu</span>
                        </a>
                    </li>
                    <li>
                        <a href="/Login/LogOut/" aria-expanded="false">
                            <i class="icon-close menu-icon"></i><span class="nav-text">Çıkış Yap</span>
                        </a>
                    </li>
                   
                </ul>
            </div>
        </div>
        <div class="content-body">
            <div class="container-fluid">
                <div class="row justify-content-center">
                    <div class="col-lg-12">
                        <div class="card">
                            <div class="card-body">
                                @RenderBody()









                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        
    </div>
    <script src="~/adminweb/plugins/common/common.min.js"></script>
    <script src="~/adminweb/js/custom.min.js"></script>
    <script src="~/adminweb/js/settings.js"></script>
    <script src="~/adminweb/js/gleek.js"></script>
    <script src="~/adminweb/js/styleSwitcher.js"></script>
    <script src="~/adminweb/plugins/validation/jquery.validate.min.js"></script>
    <script src="~/adminweb/plugins/validation/jquery.validate-init.js"></script>
</body>

</html>
